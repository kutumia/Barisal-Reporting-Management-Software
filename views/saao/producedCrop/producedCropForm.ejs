<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>মাঠে উৎপাদিত ফসলের তথ্য ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/saao/producedCropFormPost" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="year" class="col-md-3 control-label">অর্থবছর</label>
                                        <select id="year" name="year" required>
                                        <option value="2017">২০১৭-১৮</option>
                                        <option value="2018">২০১৮-১৯</option>
                                        <option value="2019">২০১৯-২০</option>
                                        <option value="2020" >২০২০-২১</option>
                                        <option value="2021">২০২১-২২</option>
                                        <option value="2022">২০২২-২৩</option>
                                        <option value="2023">২০২৩-২৪</option>                                         
                                            </select>
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">ফসলের নাম</label>
                                        <div class="col-md-9">
                                            <select name="name" id="name" class="form-control" required>
                                                <option value="">ফসলের নাম</option>
                                                <% data.forEach(function(row){ %>
                                                        <option value=<%= row.name %>><%= row.name %></option>
                                                <% }) %>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="production" class="col-md-3 control-label">উৎপাদন (টন)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="production" placeholder="উৎপাদন (টন)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="k21" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-২ঃ খন্দ-১ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="k21" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-২ঃ খন্দ-১ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="k22" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-২ঃ খন্দ-২ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="k22" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-২ঃ খন্দ-২ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="r1" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (রবি মৌসুমে খন্দ-১ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="r1" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (রবি মৌসুমে খন্দ-১ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="r2" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (রবি মৌসুমে খন্দ-২ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="r2" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (রবি মৌসুমে খন্দ-২ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="k11" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-১ঃ খন্দ-১ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="k11" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-১ঃ খন্দ-১ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="k12" class="col-md-3 control-label">আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-১ঃ খন্দ-২ মৌসুমে)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="k12" placeholder="আবাদকৃত জমির পরিমাণ (হেঃ) (খরিফ-১ঃ খন্দ-২ মৌসুমে)" value=0>
                                        </div>
                                    </div>

                                 

                                    
                                   
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                            <input type="hidden" class="form-control" name="upazilla_id" placeholder="id" value="<%= upazilla_id %>">
                                        
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>