                                        <tr>
                                            <th rowspan="2">ক্রমিক নং</th>
                                            <th rowspan="2">ফসলের নাম</th>
                                            <th colspan="2">২০১৭-১৮</th>
                                            <th colspan="2">২০১৮-১৯</th>
                                            <th colspan="2">২০১৯-২০</th>
                                            <th colspan="2">২০২০-২১</th>
                                            <th colspan="2">২০২১-২২</th>
                                            <th colspan="2">২০২২-২৩</th>
                                        </tr>  
                                        <tr>
                                            <th>আয়তন (হেঃ)</th>
                                            <th>উৎপাদন (টন)</th>
                                            <th>আয়তন (হেঃ)</th>
                                            <th>উৎপাদন (টন)</th>
