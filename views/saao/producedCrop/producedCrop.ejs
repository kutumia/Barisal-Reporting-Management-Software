                                            <th>আয়তন (হেঃ)</th>
                                            <th>উৎপাদন (টন)</th>
                                            <th>আয়তন (হেঃ)</th>
                                            <th>উৎপাদন (টন)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                                        <% cropList.forEach(function(rows,index){ %>
                                            <% let sumAreaShotero = 0 ;
                                            let sumProductionShotero = 0 ;
                                            let sumAreaAtharo = 0 ;
                                            let sumProductionAtharo = 0 ;
                                            let sumAreaUnish = 0 ;
                                            let sumProductionUnish = 0 ;
                                            let sumAreaBish = 0 ;
                                            let sumProductionBish = 0 ;
                                            let sumAreaEkush = 0 ;
                                            let sumProductionEkush = 0 ;
                                            let sumAreaBaish = 0 ;
                                            let  sumProductionBaish = 0 ;
                                            %>
                                            <tr>
                                                <td><%= index+1 %></td>
                                                <td><%= rows.name %></td>
                                                <% if (seventeen.length > 0 ){
                                                    seventeen.forEach(function(seventeens){ 
                                                    if (seventeens.crop === rows.name ){ 
                                                        sumAreaShotero = parseInt(seventeens.k21)+parseInt(seventeens.k22)+parseInt(seventeens.r1)+parseInt(seventeens.r2)+parseInt(seventeens.k11)+parseInt(seventeens.k12) 
                                                        sumProductionShotero = seventeens.production ;
                                                        console.log("sumProductionShotero",sumProductionShotero,sumAreaShotero);
                                                    
                                                    }
                                                    })
                                                }%>
                                                <td><%= sumAreaShotero %></td>
                                                <td><%= sumProductionShotero %></td>
                                                <% if (eighteen.length > 0){
                                                    eighteen.forEach(function(eighteens){ 
                                                    if (eighteens.crop === rows.name ){ 
                                                        sumAreaAtharo = parseInt(eighteens.k21)+parseInt(eighteens.k22)+parseInt(eighteens.r1)+parseInt(eighteens.r2)+parseInt(eighteens.k11)+parseInt(eighteens.k12) ;
                                                        sumProductionAtharo = eighteens.production ;
                                                        }
                                                })}
                                                


                                                if (nineteen.length > 0){
                                                    nineteen.forEach(function(nineteens){ 
                                                    if (nineteens.crop === rows.name ){ 
                                                        sumAreaUnish = parseInt(nineteens.k21)+parseInt(nineteens.k22)+parseInt(nineteens.r1)+parseInt(nineteens.r2)+parseInt(nineteens.k11)+parseInt(nineteens.k12) ;
                                                        sumProductionUnish = nineteens.production ;
                                                        }
                                                })
                                                }


                                                if (twenty.length > 0){
                                                    twenty.forEach(function(twentys){ 
                                                    if (twentys.crop === rows.name ){ 
                                                        sumAreaBish = parseInt(twentys.k21)+parseInt(twentys.k22)+parseInt(twentys.r1)+parseInt(twentys.r2)+parseInt(twentys.k11)+parseInt(twentys.k12) ;
                                                        sumProductionBish = twentys.production ;
                                                        }
                                                })
                                                }


                                                 if (twentyOne.length > 0){
                                                    twentyOne.forEach(function(twentyOnes){ 
                                                    if (twentyOnes.crop === rows.name ){ 
                                                        sumAreaEkush = parseInt(twentyOnes.k21)+parseInt(twentyOnes.k22)+parseInt(twentyOnes.r1)+parseInt(twentyOnes.r2)+parseInt(twentyOnes.k11)+parseInt(twentyOnes.k12) ;
                                                        sumProductionEkush = twentyOnes.production ;
                                                        }
                                                })
                                                }

                                                if (twentyTwo.length > 0){
                                                    twentyTwo.forEach(function(twentyTwos){ 
                                                    if (twentyTwos.crop === rows.name ){ 
                                                        sumAreaBaish = parseInt(twentyTwos.k21)+parseInt(twentyTwos.k22)+parseInt(twentyTwos.r1)+parseInt(twentyTwos.r2)+parseInt(twentyTwos.k11)+parseInt(twentyTwos.k12);
                                                        sumProductionBaish = twentyTwos.production ; 
                                                        }
                                                })
                                                }
                                                
                                                %>
                                                
                                                <td><%= sumAreaAtharo %></td>
                                                <td><%= sumProductionAtharo %></td>
                                                <td><%= sumAreaUnish %></td>
                                                <td><%= sumProductionUnish %></td>
                                                <td><%= sumAreaBish %></td>
                                                <td><%= sumProductionBish %></td>
                                                <td><%= sumAreaEkush %></td>
                                                <td><%= sumProductionEkush %></td>
                                                <td><%= sumAreaBaish %></td>
                                                <td><%= sumProductionBaish %></td>
                                                
                                                         
                                                        
                                                        
                                                        
                                                        
                                                        
                                                        
                                              
                                            </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                                <% }else{ %>
                                    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
                                        <thead>
                                            <tr>
                                                <th rowspan="2">ক্রমিক নং</th>
                                                <th rowspan="2">ফসলের নাম</th>
                                                <th colspan="2">২০১৭-১৮</th>
                                                <th colspan="2">২০১৮-১৯</th>
