<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>আবাদী জমি ও ফসল উৎপাদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/abadiJomiFormPost" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    <div class="form-group">
                                        <label for="ayoton" class="col-md-3 control-label">মোট আয়তন(ব.কি.মি)</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="ayoton" placeholder="মোট আয়তন(ব.কি.মি)">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="purush" class="col-md-3 control-label">পুরুষ জনসংখ্যা</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="purush" placeholder="পুরুষ জনসংখ্যা">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="mohila" class="col-md-3 control-label">মহিলা জনসংখ্যা</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="mohila" placeholder="মহিলা জনসংখ্যা">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="poribar" class="col-md-3 control-label">কৃষক পরিবারের সংখ্যা</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="poribar" placeholder="কৃষক পরিবারের সংখ্যা">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="block" class="col-md-3 control-label">কৃষি ব্লকের সংখ্যা</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="block" placeholder="কৃষি ব্লকের সংখ্যা">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="abadjoggo" class="col-md-3 control-label">আবাদযোগ্য জমি (হে.)</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="abadjoggo" placeholder="আবাদযোগ্য জমি (হে.)">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="abadi" class="col-md-3 control-label">আবাদি জমি (হেক্টর)</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="abadi" placeholder="আবাদি জমি (হেক্টর)">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="ek" class="col-md-3 control-label">এক ফসলী</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="ek" placeholder="এক ফসলী">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="dui" class="col-md-3 control-label">দুই ফসলী</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="dui" placeholder="দুই ফসলী">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="tin" class="col-md-3 control-label">তিন ফসলী</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="tin" placeholder="তিন ফসলী">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="cholti" class="col-md-3 control-label">চলতি পতিত (মোট)</label>
                                        <div class="col-md-9">
                                        <input type="number" class="form-control" name="cholti" placeholder="চলতি পতিত (মোট)">
                                        </div>
                                    </div>

                                    

                                   

                                    <div class="form-group">
                                        <label for="year" class="col-md-3 control-label">বছর</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="year" placeholder="year">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="year" class="col-md-3 control-label">userid</label>
                                        <div class="col-md-9">
                                            <input type="number" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>