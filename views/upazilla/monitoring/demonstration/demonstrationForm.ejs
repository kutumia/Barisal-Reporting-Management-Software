<%- include('../../../includesMonitoring/header')-%>
        <div class="wrapper">
          <%- include('../../../includesMonitoring/topbar')-%>
            <div class="page-wrap">
              <%- include('../../../includesMonitoring/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>প্রদর্শনীর তথ্য ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/demonstrationFormPost" id="FieldTrialForm" class="form-horizontal" method="post"  role="form" enctype="multipart/form-data">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                            
                                    
                                    <div class="form-group">
                                            <label for="batch" class="col-md-3 control-label">প্রদর্শনীর ব্যাচ নং</label>
                                        <select id="batch" name="batch" required>
                                        <option value="1">০১</option>
                                        <option value="2">০২</option>
                                        <option value="3">০৩</option>
                                        <option value="4" >০৪</option>
                                        <option value="5">০৫</option>
                                        <option value="6">০৬</option>
                                        <option value="7">০৭</option>                                         
                                            </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="description" class="col-md-3 control-label">প্রদর্শনীর বিস্তারিত</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="description" placeholder="প্রদর্শনীর বিস্তারিত">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="date" class="col-md-3 control-label">প্রদর্শনীর তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="date" placeholder="প্রদর্শনীর তারিখ">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label >Choose</label>
                                        <input type="file" id="filename" class="form-control" name="newsUp" required>
                                    </div>
                                    
                                    <div class="form-group">
                                            <label for="year" class="col-md-3 control-label">বছর</label>
                                        <select id="year" name="year" required>
                                        <option value="2017">২০১৭-১৮</option>
                                        <option value="2018">২০১৮-১৯</option>
                                        <option value="2019">২০১৯-২০</option>
                                        <option value="2020" >২০২০-২১</option>
                                        <option value="2021">২০২১-২২</option>
                                        <option value="2022">২০২২-২৩</option>
                                        <option value="2023">২০২৩-২৪</option>                                         
                                            </select>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../../includesMonitoring/footer')-%>