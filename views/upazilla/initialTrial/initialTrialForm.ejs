<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>প্রদর্শনীর প্রাথমিক প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/initialTrialFormPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">কৃষকের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="name" placeholder="কৃষকের নাম" value="<%=records.name%>" readonly>
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="fname" class="col-md-3 control-label">পিতার নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="fname" placeholder="পিতার নাম" value="<%=records.fname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="vname" class="col-md-3 control-label">গ্রামের/মাঠের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="vname" placeholder="গ্রামের/মাঠের নাম" value="<%=records.vname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="nid" class="col-md-3 control-label">কৃষকের এনআইডি নং</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="nid" placeholder="কৃষকের এনআইডি নং" value="<%=records.nid%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="mnum" class="col-md-3 control-label">কৃষকের মোবাইল নং</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="mnum" placeholder="কৃষকের মোবাইল নং" value="<%=records.mnum%>" readonly>
                                        </div>
                                    </div>

                                    

                                    <div class="form-group">
                                        <label for="cdistribution" class="col-md-3 control-label">প্রকল্প শুরুর পূর্বের শস্যবিন্যাস (রবি-খ,১- খ২)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="cdistribution" placeholder="প্রকল্প শুরুর পূর্বের শস্যবিন্যাস (রবি-খ,১- খ২)" value="<%=records.cdistribution%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="pdistribution" class="col-md-3 control-label">প্রস্তাবিত শস্যবিন্যাস (রবি-খ,১- খ২)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pdistribution" placeholder="প্রস্তাবিত শস্যবিন্যাস (রবি-খ,১- খ২)" value="<%=records.pdistribution%>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="ptype" class="col-md-3 control-label">প্রদর্শনীর ধরণ(একক/ব্লক/সবজি/ফল/মিশ্রফল/বিশেষ)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="ptype" placeholder="প্রদর্শনীর ধরণ(একক/ব্লক/সবজি/ফল/মিশ্রফল/বিশেষ)" value="<%=records.ptype%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="tname" class="col-md-3 control-label">প্রদর্শনী ফসলের/প্রযুক্তির নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pname" placeholder="প্রদর্শনী ফসলের/প্রযুক্তির নাম" value="<%=records.pname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="breedname" class="col-md-3 control-label">জাতের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="breedname" placeholder="জাতের নাম" value="<%=records.breedname%>">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="source">বীজ/চারার উৎস:</label>
                                        <select id="source" name="source">
                                            <option value="<%=records.source%>" > <%=records.source%></option>
                                            <option value="বাংলাদেশ কৃষি গবেষণা ইনস্টিউট">বাংলাদেশ কৃষি গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ ধান গবেষণা ইনস্টিউট">বাংলাদেশ ধান গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ পাট গবেষণা ইনস্টিউট">বাংলাদেশ পাট গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ পরমাণু গবেষণা ইনস্টিউট">বাংলাদেশ পরমাণু গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ কৃষি উন্নয়ন কর্পোরেশন">বাংলাদেশ কৃষি উন্নয়ন কর্পোরেশন</option>
                                        </select>
                                            </div>
                                    </div>
                                       
                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="trialsize">প্রদর্শনীর আকার বাছুন:</label>
                                        <select id="trialsize" name="trialsize" required>
                                            <option value="১০ শতক">১০ শতক</option>
                                            <option value="২০ শতক">২০ শতক</option>
                                            <option value="৩৩ শতক">৩৩ শতক</option>
                                            <option value="১ একর">১ একর</option>
                                        </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="trialdate" class="col-md-3 control-label">প্রদর্শনীর বপন/রোপণ/স্থাপনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="trialdate" placeholder="প্রদর্শনীর বপন/রোপণ/স্থাপনের তারিখ" value="<%=records.trialdate%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="block" class="col-md-3 control-label">ব্লকের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="block" placeholder="ব্লকের নাম" value="<%=records.block%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="saooname" class="col-md-3 control-label">এসএএওর নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="saooname" placeholder="এসএএওর নাম" value="<%=records.saaoname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="pnum" class="col-md-3 control-label">এসএএওর ফোন নাম্বার</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pnum" placeholder="এসএএওর ফোন নাম্বার" value="<%=records.pnum%>" readonly>
                                        </div>
                                    </div>

                                    <input type="hidden" class="form-control" name="year" placeholder="বছর" value="<%=records.year%>">

                                    
                                    <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= records.upazilla_id %>">
                                        
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>