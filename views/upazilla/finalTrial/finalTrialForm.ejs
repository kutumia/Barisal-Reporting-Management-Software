<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>প্রদর্শনীর চূড়ান্ত প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/finalTrialFormPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    <div class="form-group">
                                        <label for="name" class="col-md-3 control-label">কৃষকের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="name" placeholder="কৃষকের নাম" value="<%=records.name%>" readonly>
                                        </div>
                                        
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="fname" class="col-md-3 control-label">পিতার নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="fname" placeholder="পিতার নাম" value="<%=records.fname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="vname" class="col-md-3 control-label">গ্রামের/মাঠের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="vname" placeholder="গ্রামের/মাঠের নাম" value="<%=records.vname%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="nid" class="col-md-3 control-label">কৃষকের এনআইডি নং</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="nid" placeholder="কৃষকের এনআইডি নং" value="<%=records.nid%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="mnum" class="col-md-3 control-label">কৃষকের মোবাইল নং</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="mnum" placeholder="কৃষকের মোবাইল নং" value="<%=records.mnum%>" readonly>
                                        </div>
                                    </div>

                                    

                                    <div class="form-group">
                                        <label for="cdistribution" class="col-md-3 control-label">প্রকল্প শুরুর পূর্বের শস্যবিন্যাস (রবি-খ,১- খ২)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="cdistribution" placeholder="প্রকল্প শুরুর পূর্বের শস্যবিন্যাস (রবি-খ,১- খ২)" value="<%=records.cdistribution%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="pdistribution" class="col-md-3 control-label">প্রস্তাবিত শস্যবিন্যাস (রবি-খ,১- খ২)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pdistribution" placeholder="প্রস্তাবিত শস্যবিন্যাস (রবি-খ,১- খ২)" value="<%=records.pdistribution%>" readonly>
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="tname" class="col-md-3 control-label">প্রদর্শনীর ধরণ(একক/ব্লক/সবজি/ফল/মিশ্রফল/বিশেষ)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="ptype" placeholder="প্রদর্শনীর ধরণ(একক/ব্লক/সবজি/ফল/মিশ্রফল/বিশেষ)" value="<%=records.ptype%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="tname" class="col-md-3 control-label">প্রদর্শনী ফসলের/প্রযুক্তির নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="pname" placeholder="প্রদর্শনী নাম/প্রযুক্তির নাম" value="<%=records.pname%>" readonly>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="breedname" class="col-md-3 control-label">জাতের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="breedname" placeholder="জাতের নাম" value="<%=records.breedname%>" readonly>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="source" class="col-md-3 control-label">বীজ/চারার উতস(গবেষণা প্রতিষ্ঠানের নাম/বিএসডিসি/কোম্পানীর নাম)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="source" placeholder="বীজ/চারার উতস(গবেষণা প্রতিষ্ঠানের নাম/বিএসডিসি/কোম্পানীর নাম)" value="<%=records.source%>" readonly>
                                        </div>
                                    </div>
                                       
                                    <div class="form-group">
                                        <label for="trialsize" class="col-md-3 control-label">প্রদর্শনীর আকার(শতক)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="trialsize" placeholder="প্রদর্শনীর আকার(শতক)" value="<%=records.trialsize%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="trialdate" class="col-md-3 control-label">প্রদর্শনীর বপন/রোপণ/স্থাপনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="trialdate" placeholder="প্রদর্শনীর বপন/রোপণ/স্থাপনের তারিখ" value="<%=records.trialdate%>" readonly>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="cdate" class="col-md-3 control-label">কর্তনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="cdate" placeholder="কর্তনের তারিখ" value="<%=records.cdate%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="production" class="col-md-3 control-label">উৎপাদন(কেজি/প্রদর্শনী)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="production" placeholder="উৎপাদন(কেজি/প্রদর্শনী)" value="<%=records.production%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="folon" class="col-md-3 control-label">ফলন(টন/হেক্টর)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="folon" placeholder="ফলন(টন/হেক্টর)" value="<%=records.folon%>">
                                        </div>
                                    </div> 

                                    <div class="form-group">
                                        <label for="bij" class="col-md-3 control-label">সংরক্ষিত বীজের পরিমাণ(কেজি)</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="bij" placeholder="সংরক্ষিত বীজের পরিমাণ(কেজি)" value="<%=records.bij%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="comment" class="col-md-3 control-label">মন্তব্য</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="comment" placeholder="মন্তব্য" value="<%=records.comment%>">
                                        </div>
                                    </div>

                                   
                                    <input type="hidden" class="form-control" name="year" placeholder="বছর" value="<%=records.year%>">

                                    
                                   
                                            <input type="hidden" class="form-control" name="id" placeholder="id" value="<%=records.upazilla_id%>">
                                      
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>