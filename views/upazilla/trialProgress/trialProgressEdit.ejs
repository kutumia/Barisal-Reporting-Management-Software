<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>প্রদর্শনীর অগ্রগতির প্রতিবেদন ফর্ম</h2></div>
                                <p><%                                                                                                 
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/trialProgressEditPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    
                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="name">প্রদর্শনীর/কার্যক্রমের নাম বাছুন:</label>
                                        <select id="name" name="name" required>
                                            <option value=<%=records.name%>><%=records.name%></option>
                                            <option value="ধান (আউশ)">ধান (আউশ)</option>
                                            <option value="ধান (আমন)">ধান (আমন)</option>
                                            <option value="গম">গম</option>
                                            <option value="ভূট্টা">ভূট্টা</option>
                                            <option value="বার্লি">বার্লি</option>
                                            <option value="পাট">পাট</option>
                                            <option value="মুগ">মুগ</option>
                                            <option value="খেসারি">খেসারি</option>
                                            <option value="ফেলন">ফেলন</option>
                                            <option value="সরিষা">সরিষা</option>
                                            <option value="তিল">তিল</option>
                                            <option value="সয়াবিন">সয়াবিন</option>
                                            <option value="চীনাবাদাম">চীনাবাদাম</option>
                                            <option value="সূর্যমূখী">সূর্যমূখী</option>
                                            <option value="পিঁয়াজ">পিঁয়াজ</option>
                                            <option value="রসুন">রসুন</option>
                                            <option value="হলুদ">হলুদ</option>
                                            <option value="আলু">আলু</option>
                                            <option value="বেগুন">বেগুন</option>
                                            <option value="টমেটো">টমেটো</option>
                                            <option value="ঢেঁড়স">ঢেঁড়স</option>
                                            <option value="পুঁইশাক">পুঁইশাক</option>
                                            <option value="ডাটা">ডাটা</option>
                                            <option value="কুমড়া জাতীয়">কুমড়া জাতীয়</option>
                                            <option value="সীম">সীম</option>
                                            <option value="বরবটি">বরবটি</option>
                                            <option value="আম">আম</option>
                                            <option value="পেয়ারা">পেয়ারা</option>
                                            <option value="কলা">কলা</option>
                                            <option value="পেঁপে">পেঁপে</option>
                                            <option value="সফেদা">সফেদা</option>
                                            <option value="মাল্টা">মাল্টা</option>
                                            <option value="কুল">কুল</option>
                                            <option value="তরমুজ">তরমুজ</option>
                                        </select>
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="boraddo" class="col-md-3 control-label">বরাদ্দ</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="boraddo" placeholder="বরাদ্দ" value="<%=records.boraddo%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                            <label for="done" class="col-md-3 control-label">স্থাপিত/সম্পন্ন</label>
                                        <select id="done" name="done">
                                            <option value=<%=records.done%>><%=records.done%></option>
                                            <option value="স্থাপিত">স্থাপিত</option>
                                            <option value="সম্পন্ন">সম্পন্ন</option>
                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="breed" class="col-md-3 control-label">প্রদর্শনীর ফসলের জাতের নাম(প্রদরশনীর সংখ্যা ভিত্তিক)</label>
                                        <div class="col-md-9">
                                        <input type="text" class="form-control" name="breed" placeholder="প্রদর্শনীর ফসলের জাতের নাম(প্রদরশনীর সংখ্যা ভিত্তিক)" value="<%=records.breed%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="source">বীজ/চারার উৎস:</label>
                                        <select id="source" name="source">
                                            <option value=<%=records.source%>><%=records.source%></option>
                                            <option value="বাংলাদেশ কৃষি গবেষণা ইনস্টিউট">বাংলাদেশ কৃষি গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ ধান গবেষণা ইনস্টিউট">বাংলাদেশ ধান গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ পাট গবেষণা ইনস্টিউট">বাংলাদেশ পাট গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ পরমাণু গবেষণা ইনস্টিউট">বাংলাদেশ পরমাণু গবেষণা ইনস্টিউট</option>
                                            <option value="বাংলাদেশ কৃষি উন্নয়ন কর্পোরেশন">বাংলাদেশ কৃষি উন্নয়ন কর্পোরেশন</option>
                                        </select>
                                            </div>
                                    </div>
                                    

                                   

                                    <input type="hidden" class="form-control" name="year" placeholder="বছর" value="<%=records.year%>">


                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>